<Window x:Class="RappFinexx.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RappFinexx"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    
        <StackPanel Margin="0,10,650.2,-0.2">
        <Grid>
                    <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>

        <Expander Margin="5">
            <TextBlock Text="expander 1 content" />
        </Expander>
        <Expander Grid.Row="1" Margin=" 5">
            <TextBlock Text="expander 2 content" />
        </Expander>
        <Expander Grid.Row="3" Margin="5">
            <TextBlock Text="expander 3 content" />
        </Expander>
        </Grid>
    </StackPanel>


</Window>
